<Window x:Name="mnWindow" x:Class="PrintIndex.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Print From Index" Height="500" Width="223" MinWidth="258" MinHeight="260" Background="#FFE1E2E3">
    <Grid>
        <ListBox x:Name="ScheduleListBox" Margin="10,52,15,129"/>
        <Button Content="OK" Margin="0,0,15,18" Click="Button_Click" Height="22" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75"/>
        <TextBox x:Name="TextBox1" Margin="10,0,15,45" TextWrapping="Wrap" Text="SheetIndex" Height="23" VerticalAlignment="Bottom"/>
        <Label Content="Select a Sheet Index schedule &#xD;&#xA;to print from:" HorizontalAlignment="Left" Margin="6,2,0,0" VerticalAlignment="Top" FontSize="14" FontWeight="Bold"/>
        <Label Content="Enter a name to save the set as.&#xA;It will be available in the print dialog:" HorizontalAlignment="Left" Margin="5,0,0,73" FontSize="14" Height="47" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
